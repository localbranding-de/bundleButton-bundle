{
	"name" : "localbranding-de/bundlebutton-bundle",
	"type" : "contao-bundle",
	"version" : "1.0.7",
	"description" : "LocalBRanding Extension for Team Site",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Localbranding-de",
			"homepage" : "https://github.com/localbranding-de"
		}
	],
	"conflict" : {
		"contao/core" : "*",
		"contao/manager-plugin" : "<2.0 || >=3.0"
	},
	"require-dev" : {
		"contao/manager-plugin" : "^2.0",
		"doctrine/doctrine-cache-bundle" : "^1.3",
		"friendsofphp/php-cs-fixer" : "^2.6",
		"leofeyer/optimize-native-functions-fixer" : "^1.1",
		"php-http/guzzle6-adapter" : "^1.1",
		"php-http/message-factory" : "^1.0.2",
		"phpunit/phpunit" : "^5.7.26",
		"symfony/phpunit-bridge" : "^3.2"
	},
	"extra" : {
		"contao-manager-plugin" : "LocalbrandingDe\\BundleButtonBundle\\ContaoManager\\Plugin"
	},
	"autoload" : {
		"psr-4" : {
			"LocalbrandingDe\\BundleButtonBundle\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"LocalbrandingDe\\BundleButtonBundle\\Tests\\" : "tests/"
		}
	},
	"support" : {
		"issues" : "https://github.com/localbranding-de/siteteam_bundle/issues",
		"source" : "https://github.com/localbranding-de/siteteam_bundle"
	}
}
